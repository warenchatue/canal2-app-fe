{
  "private": true,
  "name": "dinoes",
  "version": "1.1.2",
  "type": "module",
  "main": "./nuxt.config.ts",
  "engines": {
    "node": ">=16.15",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev": "nuxi dev .app --open",
    "build": "nuxi build .app",
    "clean:all": "pnpm -r clean && rimraf .nuxt .output node_modules",
    "test": "run-p test:*",
    "test:lint": "run-s lint:eslint lint:prettier",
    "lint": "run-s lint:eslint:fix lint:eslint:fix-demo lint:prettier:fix",
    "lint:prettier": "prettier --check \"./**/*.(ts|vue|css|md)\"",
    "lint:prettier:fix": "prettier --write \"./**/*.(ts|vue|css|md)\"",
    "lint:eslint": "eslint -c .eslintrc.cjs --ext .vue,.ts .",
    "lint:eslint:fix": "eslint -c .eslintrc.cjs --fix --ext .vue,.ts .",
    "lint:eslint:fix-start": "eslint -c layers/documentation/examples/.eslintrc.cjs --fix --ext .vue layers/documentation/examples"
  },
  "devDependencies": {
    "@iconify/json": "2.2.67",
    "@parcel/watcher": "2.1.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@types/fs-extra": "11.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier-vue": "4.2.0",
    "eslint-plugin-tailwindcss": "3.12.0",
    "eslint-plugin-vue": "9.13.0",
    "eslint-plugin-vuejs-accessibility": "2.1.0",
    "esno": "0.16.3",
    "fs-extra": "11.1.1",
    "lint-staged": "13.2.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "std-env": "3.3.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vite-plugin-node-polyfills": "^0.15.0",
    "vue-tsc": "1.6.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.(ts|vue)": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss",
        "vue",
        "vite",
        "@unhead/vue",
        "webpack"
      ]
    },
    "overrides": {
      "consola": "3.1.0"
    }
  },
  "dependencies": {
    "@nuxtjs/moment": "^1.6.1",
    "@pinia/nuxt": "^0.4.11",
    "@types/events": "^3.0.2",
    "@types/formidable": "^3.4.0",
    "formidable": "^3.5.0",
    "h3-formidable": "^0.3.8",
    "moment": "^2.29.4",
    "pinia": "^2.1.6"
  }
}
